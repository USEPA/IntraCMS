 *
 * @ingroup themeable
 */
#}
{% block content %}
    <a href="http://r8net.epa.gov/" rel="home">
        <img src="/{{active_theme_path() }}/assets/img/Region8LogoRet_1_50.png" alt="{{ 'Region 8 Home'|t }}"/>
    </a>
{% endblock %}